 @using WebBDS_Project.Models
@{
   
    davisoft_bdsprojectEntities db = new davisoft_bdsprojectEntities();
  
}
<div class="right_col">
                            <h3>Việc làm bất động sản</h3>
                            <ul class="last_list_right">
                                
                                
                                @{
                                    foreach (var item in db.BDSCareers.Where(T => T.Active == 1).ToList())
                                    {
                                           <li>
                                        <a href="@Url.Action("Search", "Default", new { filterCareer = item.ID })">@item.Name</a>
                                        </li>
                                    }
                                }
                                

                               
                            </ul>
                        </div>
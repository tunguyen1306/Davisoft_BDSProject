<script>
    $(function () {
        $('.select2').select2();
        $(".datepicker").datepicker({
            showOn: "button",
            buttonImage: "/Template/images/calendar.gif",
            buttonImageOnly: true,
            buttonText: "Select date"
        });
        $('#hdValuesDate').change(function () {
            var birthday = $('#hdValuesDate').val().split('/');
            $('#TblBDSPersonalInformation_Birthday_Day').val(birthday[1]);
            $('#TblBDSPersonalInformation_Birthday_Month').val(birthday[0]);
            $('#TblBDSPersonalInformation_Birthday_Year').val(birthday[2]);

        });
        $("#TblBDSPersonalInformation_Birthday_Year").change(function () {

            var datet = $('#TblBDSPersonalInformation_Birthday_Day').val() + "/" + $('#TblBDSPersonalInformation_Birthday_Month').val() + "/" + $('#TblBDSPersonalInformation_Birthday_Year').val();
            $('#hdValuesDate').val(datet);
        });
    });
    $("#submitbutton").on('click', function (e) {
        e.preventDefault();
        $('#submitbutton').parents("form").submit();
    });
    $("#submitbutton1").on('click', function (e) {
        e.preventDefault();
        $('#submitbutton1').parents("form").submit();
    });
    $('.btnCheck').click(function () {
        CheckEmail();
    });
    $('.btnChangePass').click(function () {
        ChangePass();
    });
    function CheckEmail() {
        var t = 0;
        if ($('#TblBdsAdcount_Email').val() == "") {
            toastr.error('Vui lòng nhập Email');
            t++;
        }
        if (t == 0) {
            var obj = {};
            obj.Email = $('#TblBdsAdcount_Email').val();
            $.ajax({
                type: "POST",
                url: '@Url.Action("CheckEmail", "Management")',
                data: JSON.stringify(obj),
                dataType: "json",
                contentType: "application/json;charset=utf-8",
                success: function (data) {
                    if (data.result == 1) {
                        $('.showCheck').addClass('hidden');
                        $('.showFormPass').removeClass('hidden');
                    }
                    if (data.result == 0) {
                        toastr.error('Email không khớp với tài khoản đang đăng nhập!');
                    }


                }

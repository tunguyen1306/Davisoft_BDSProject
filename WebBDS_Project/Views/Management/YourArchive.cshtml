@{
    ViewBag.Title = "YourArchive";
    Layout = "~/Views/Shared/_LayoutWeb.cshtml";
}

@model WebBDS_Project.Models.RegisterInformationModel

<div class="container">
    <div class="grid_9 main_left">
        <div class="group_work">
            <div class="header_text">
                <h3><span><i class="fa fa-list-ul"></i>Hồ sơ đã lưu</span></h3>
            </div>
            <div class="grid_12">
                <table class="table-responsive" style="margin-top: 10px;">
                    <thead>
                        <tr>
                            <th data-field="id">NGÀY TÌM VIỆC</th>
                            <th data-field="name">HÌNH ẢNH</th>
                            <th data-field="name">HỌ VÀ TÊN</th>
                            <th data-field="name">GIỚI TÍNH</th>
                            <th data-field="name">NĂM SINH</th>
                            <th data-field="name">KINH NGHIỆM</th>
                   
                  
                            <th data-field="name">VỊ TRÍ MONG MUỐN</th>
                            <th data-field="name">TỈNH / THÀNH PHỐ</th>
                            <th data-field="name">QUẬN HUYỆN</th>
                            <th data-field="name">HỌC VẤN</th>
                            <th data-field="name">CHI TIẾT</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in Model.Listbdspersonalinformation)
                        {
                            <tr>
                            <td>@item.CreateDate</td>
                            <td><img width="50" src="@System.Configuration.ConfigurationManager.AppSettings["UrlImage"]@item.UrlImage.ToString().Split('_')[0]/@item.UrlImage" /></td>
                            <td>@item.Name</td>
                            <td>@item.Sex</td>
                            <td>@item.Birthday</td>
                            <td>@item.Experience</td>
                            <td>@item.IdLoaiNghe</td>
                            <td>@item.City</td>
                            <td>@item.District</td>
                            <td>@item.Education</td>
                            <td>xem</td>
                        </tr> 
                        }
                        
                    </tbody>
                </table>
            </div>

        </div>
    </div>
     <div class="grid_3 omega main_right">
           
            @Html.Partial("_partialManagerCenter")

        </div>
</div>

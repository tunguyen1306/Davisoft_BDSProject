@using WebBDS_Project.Models
@{

    davisoft_bdsprojectEntities1 db = new davisoft_bdsprojectEntities1();

}
<div class="right_col" >
    <h3>Tìm kiếm theo kinh nghiệm</h3>
    <ul class="last_list_right filter" style="">


        @{
            var checklist = Request["filterTimeWorking"] != null ? Request["filterTimeWorking"].ToString().Split(',').Select(T => T) : new String[0];
            foreach (var item in db.BDSTimeWorks.OrderBy(T=>T.Index).Where(T=>T.Active==1).ToList())
            {
                <li>
                    <a><input @(checklist.Contains(item.KeyUrl) ? "checked='checked'" : "") class="check_filter" type="radio" name="filterTimeWorking" value="@item.KeyUrl" /> <label>@item.Name</label></a>
                </li>
            }
        }



    </ul>
</div>
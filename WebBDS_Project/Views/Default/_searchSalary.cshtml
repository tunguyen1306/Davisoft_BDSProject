@using WebBDS_Project.Models
@{

    davisoft_bdsprojectEntities1 db = new davisoft_bdsprojectEntities1();

}
<div class="right_col">
    <h3>Tìm kiếm theo mức lương</h3>
    <ul class="last_list_right filter" style="">


        @{
            var checklist = ViewData["filterSalary"] != null ? ViewData["filterSalary"].ToString().Split(',').Select(T => T) : new String[0];
            foreach (var item in db.BDSSalaries.Where(T=>T.Active==1).ToList())
            {
                <li>
                    <a><input class="check_filter" @(checklist.Contains(item.KeyUrl) ? "checked='checked'" : "") type="radio" name="filterSalary" value="@item.KeyUrl" /> <label>@item.Name</label></a>
                </li>
            }
        }



    </ul>
</div>